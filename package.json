{
  "name": "lambda-catfish-photos",
  "version": "1.0.0",
  "description": "My personal photography website powered by Hugo and Bridget theme",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all -p \"build:theme-assets\" \"hugo:server\"",
    "build:theme-assets": "pnpm --dir themes/bridget run vite:dev",
    "hugo:server": "hugo server --disableFastRender --watch --logLevel info",
    "hugo:build": "hugo --minify"
  },
  "keywords": [
    "hugo",
    "photography",
    "bridget"
  ],
  "author": "Jack",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}